<launch>
  <!-- launching Arduino robot node -->
  <node pkg="rosserial_python" type="serial_node.py" name="motorarduino_serial_node">
    <param name="port" value="/dev/motorarduino"/>
    <param name="baud" value="115200"/>
  </node>

<node pkg="rosserial_python" type="serial_node.py" name="gasarduino_serial_node">
    <param name="port" value="/dev/gasarduino"/>
    <param name="baud" value="115200"/>
  </node>


  <rosparam command="load" file="$(find hongik_bringup)/config/motor.yaml" />

</launch>
